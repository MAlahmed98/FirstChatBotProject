import java.util.Scanner;
public class prototype{
public static void main(String [] args){
    Scanner k = new Scanner(System.in);
    String msg = "";
    String response = "Hello Human! This is just a simple prototype, by a simple programmer who wants to do great things someday, if you wish to stop me type end, now, what can i do for you?";
    System.out.println("ChatBot ->" + response);
    while(msg != "end"){
      System.out.print("You ->");
      msg = k.next();
       switch(msg) {
          case "hi": response = "Hello there human";
          break;
          case "Hi": response = "Hello buddy";
          break;
          case "aye": response = "aye homie";
          break;
          case "yo": response = "aye whsup";
          break;
          case "whats cracking": response = "notihng much, u?";
          break;
          case "how are you?": response = "System is fine and working, wbu?";
          break;
          case "whatsup?": response = "just processing shit around, u?";
          break;
          default : response = "i don't get it!";
          break;
          
      }
      System.out.println("ChatBot ->" + response);
    }
  }
}
